{
    "version": 2,
    "builds": [
        {
            "src": "src/app.js",
            "use": "@vercel/node",
            "config": {
                "runtime": "nodejs20.x"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "PATCH",
                "DELETE",
                "OPTIONS"
            ],
            "dest": "src/app.js"
        }
    ]
}